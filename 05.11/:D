x = 10
y = 10
xChange = 10
yChange = 10

def setup():
    global x,y,xChange,yChange
    size(800,800)
    background(75,173,97)
    stroke(255)
    frameRate(10)
    
    
def draw():
    #background(75,173,97)
    w = random (0,100)
    d = random (0,100)
    global x,y,xChange,yChange
    ellipse(x,y,w,d)
    x = x + xChange
    y = y + yChange
    fill(random(0,255),random(0,255),random(0,255))
    stroke(random(0,255),random(0,255),random(0,255))
