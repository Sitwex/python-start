angle = 0

def setup():
    global
    size(700,700)
    background(0)
    frameRate(100)
    
def draw():
    global angle
    background(0)
    ellipseMode(CENTER)
    ellipse(350,350,100,100)
    ellipseMode(CORNER)
    translate(330,330)
    rotate(radians(angle))
    angle = angle + 1
    ellipse()
    stroke(random(255),random(255),random(255))
    
