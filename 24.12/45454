x = 0
y = 0

def setup():
    global x,y,xChange,x2,y2,x3,y3,x4,y4
    size(700,700)
    background(255)
    frameRate(1)
    stroke(255)
    
def draw():
    global x,y,xChange
    background(0)
    #stroke(random(0,255),random(0,255),random(0,255))
    x = random (0,800)
    y = random (0,800)
    for step in range(10):
        point(random(x,y)
        strokeWeight(random(0,20))
