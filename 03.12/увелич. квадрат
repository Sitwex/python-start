d = 0
w = 0
dChange = 1
wChange = 1
x = 350 
y = 350
x2 = 0
y2 = 0
x3 = 0
y3 = 0
xChange = 1
yChange = 1
x2Change = 1
y2Change = 1
x3Change = 1
y3Change = 1

def setup():
    global x,y,x2,y2,x3,y3,xChange,yChange,x2Change,y2Change,x3Change,y3Change,w,d,dChange,wChange
    size (700,700)
    
def draw():
    global x,y,x2,y2,x3,y3,xChange,yChange,x2Change,y2Change,x3Change,y3Change,w,d,dChange,wChange
    rectMode(CENTER)
    rect(x,y,w,d)
    
    d = d + dChange
    w = w + wChange
    
