angle = 0
angle1 = 0

def setup():
    global angle,angle1
    size(800,800)
    background(168,216,114)
    rectMode(CORNER)

def draw():
    global angle,angle1
    push()
    translate(350,350)
    rotate(radians(angle))
    #background(168,216,114)
    angle = angle + 1
    push()
    translate(200,200)
    rotate(radians(angle1))
    rect(80,80,80,80)
    angle1 = angle1 + 3
    pop()
    pop()
