x = 0
y = 0
w = 0
d = 0

def setup():
    global x,y,w,d
    size(800,800)
    background (123,162,161)
    frameRate(110)
    
def draw():
    global x,y,w,d
    x = random (0,800)
    y = random (0,800)
    w = random (400,300)
    d = random (400,300)
    stroke(random(0,255),random(0,255),random(0,255))
    line (x,y,w,d)
            
   
